<table>
    <thead>
        <tr>
            <th>Version</th>
            <th>Released</th>
            <th>Changelog</th>
            <th colspan="2" class="desktoponly">Download</th>
            <th>Docs</th>
        </tr>
    </thead>
    <% const org = "https://github.com/NovaSheets" -%>
    <% for (let item of versions) { -%>
        <% let ver  = item[0] -%>
        <% let date = item[1] -%>
        <% let docs = item[2] -%>
        <% let hash = item[3] -%>
        <% let patch = ver.substr(4) %>
        <% let pre = ver.substr(5) %>
        <% const bg = patch === '0' ? '#0004' : (pre ? '#4444' : '#2224') %>
        <tr style="background: <%= bg %>;">
            <td><%= ver %></td>
            <td><%= date %></td>
            <td><a href="/changelog/#<%= ver.replace(/\./g, '') %>">View</a></td>
            <td class="desktoponly"><a href="/src/<%= ver %>">Full</a></td>
            <td class="desktoponly"><a href="/src/<%= ver %>/min">Min</a></td>
            <td>
                <% if (docs === "rdme") { -%>
                    <a href="<%= org %>/website/blob/<%= ver %>/readme.md">View</a>
                <% } else if (docs === "wiki") { -%>
                    <a href="<%= org %>/NovaSheets/wiki/Syntax/<%= hash %>">Wiki</a>
                <% } else if (docs === "gtag") { -%>
                    <a href="<%= org %>/website/tree/<%= ver %>/pages/docs">Browse</a>
                <% } else if (docs === "site") { -%>
                    <a href="/docs/">Site</a>
                <% } -%>
            </td>
        </tr>
    <% } -%>
</table>
